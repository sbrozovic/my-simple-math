<?xml version="1.0" encoding="utf-8"?>
<!--Part of TabLessonActivity-->
<ScrollView
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:background="@color/colorPrimary"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:auto="http://schemas.android.com/tools">
<LinearLayout
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_width="match_parent">
<android.support.constraint.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    auto:layout_editor_absoluteY="0dp"
    auto:layout_editor_absoluteX="0dp"
    android:layout_alignParentStart="true"
    android:layout_alignParentTop="true">

    <!-- tu se unosi koje ce elemente imati tab-->
    <io.github.kexanie.library.MathView
        android:id="@+id/textView_tabExample_task"
        android:layout_width="313dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:text="TextView"
        auto:engine="MathJax"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="43dp" />

    <TextView
        android:id="@+id/textView_tabExample_1"
        android:layout_width="313dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:textSize="15dp"
        android:layout_alignEnd="@+id/textView_tabExample_task"
        android:layout_below="@+id/textView_tabExample_task"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/textView_tabExample_task" />

    <TextView
        android:id="@+id/textView_tabExample_2"
        android:layout_width="313dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:textSize="15dp"
        android:layout_alignStart="@+id/textView_tabExample_1"
        android:layout_below="@+id/textView_tabExample_1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView_tabExample1_1"
        android:layout_marginTop="8dp" />

    <io.github.kexanie.library.MathView
        android:id="@+id/textView_tabExample_3"
        android:layout_width="313dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView_tabExample_2"
        android:layout_below="@+id/textView_tabExample_2"
        auto:engine="MathJax"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView_tabExample1_2" />

    <TextView
        android:id="@+id/textView_tabExample_5"
        android:layout_width="313dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="14dp"
        android:textColor="#000000"
        android:textSize="15dp"
        app:layout_constraintTop_toBottomOf="@+id/textView_tabExample_4" />

    <io.github.kexanie.library.MathView
        android:id="@+id/textView_tabExample_4"
        android:layout_width="313dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView_tabExample_3"
        android:layout_below="@+id/textView_tabExample_3"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        auto:engine="MathJax"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView_tabExample_3" />

    <io.github.kexanie.library.MathView
        android:id="@+id/mathView_tabExample_6"
        android:layout_width="313dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView_tabExample_5"
        android:layout_below="@+id/textView_tabExample_5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        auto:engine="MathJax"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView_tabExample1_3" />

    <io.github.kexanie.library.MathView
        android:id="@+id/mathView_tabExample_7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView_tabExample_1"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView_tabExample_1"
        android:layout_marginStart="8dp"></io.github.kexanie.library.MathView>

    <io.github.kexanie.library.MathView
        android:id="@+id/mathView_tabExample_8"
        android:layout_width="313dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView_tabExample_6i7"></io.github.kexanie.library.MathView>

    <ImageView
        android:id="@+id/imageView_tabExample1_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/textView_tabExample_1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageView
        android:id="@+id/imageView_tabExample1_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mathView_tabExample_8" />

    <ImageView
        android:id="@+id/imageView_tabExample1_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:color/transparent"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/textView_tabExample_2"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.501" />

    <ImageView
        android:id="@+id/imageView_tabExample1_3"
        android:layout_width="3dp"
        android:layout_height="3dp"
        android:layout_marginTop="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_tabExample_5"
        app:srcCompat="@android:color/transparent" />

    <TextView
        android:id="@+id/textView_tabExample_6i7"
        android:layout_width="313dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mathView_tabExample_6" />

</android.support.constraint.ConstraintLayout>
</LinearLayout></ScrollView>