<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sara.mysimplemath">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/AppBaseTheme">
        <activity
            android:name=".activity.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.AvailablePlayersActivity"
            android:screenOrientation="portrait"
            android:noHistory="true"/>
        <activity
            android:name=".activity.GameActivity"
            android:screenOrientation="portrait"
            android:noHistory="true"/>
        <activity
            android:name=".activity.LessonActivity"
            android:screenOrientation="portrait"
            android:noHistory="true"/>
        <activity
            android:name=".activity.ResultsOfGameActivity"
            android:screenOrientation="portrait"
            android:noHistory="true"/>
        <activity
            android:name=".activity.TabLessonActivity"
            android:screenOrientation="portrait"></activity>

        <!--<service android:name=".other.TokenService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service android:name=".other.FCMMessageReceiverService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>-->
    </application>

</manifest>